<PhysiCell_settings version="devel-version">
	<domain>
		<x_min>-1500</x_min>
		<x_max>1500</x_max>
		<y_min>-1500</y_min>
		<y_max>1500</y_max>
		<z_min>-10</z_min>
		<z_max>10</z_max>
		<dx>20</dx>
		<dy>20</dy>
		<dz>20</dz>
		<use_2D>true</use_2D>
	</domain>
	
	<overall>
		<max_time units="min">7200</max_time> 
		<time_units>min</time_units>
		<space_units>micron</space_units>	
		
		<dt_diffusion units="min">0.01</dt_diffusion>
		<dt_mechanics units="min">0.1</dt_mechanics>
		<dt_phenotype units="min">6</dt_phenotype>	
	</overall>
	
	<parallel>
		<omp_num_threads>10</omp_num_threads>
	</parallel> 
	
	<save>
		<folder>output</folder> <!-- use . for root --> 

		<full_data>
			<interval units="min">600</interval>
			<enable>true</enable>
		</full_data>
		
		<SVG>
			<interval units="min">600</interval>
			<enable>true</enable>
		</SVG>
		
		<legacy_data>
			<enable>false</enable>
		</legacy_data>
	</save>

	<microenvironment_setup>
		<variable name="oxygen" units="mmHg" ID="0">
			<physical_parameter_set>
				<diffusion_coefficient units="micron^2/min">100000.00</diffusion_coefficient>
				<decay_rate units="1/min">0.1</decay_rate>  
			</physical_parameter_set>
			<initial_condition units="mmHg">45.94</initial_condition>
			<Dirichlet_boundary_condition units="mmHg" enabled="true">45.94</Dirichlet_boundary_condition>
		</variable>
		
		<options>
			<calculate_gradients>true</calculate_gradients>
			<track_internalized_substrates_in_each_agent>false</track_internalized_substrates_in_each_agent>
			<!-- not yet supported --> 
			<initial_condition type="matlab" enabled="false">
				<filename>./config/initial.mat</filename>
			</initial_condition>
			<!-- not yet supported --> 
			<dirichlet_nodes type="matlab" enabled="false">
				<filename>./config/dirichlet.mat</filename>
			</dirichlet_nodes>
		</options>
	</microenvironment_setup>
	
	<user_parameters>	    
		<persitence_timeHip type="double" units="min" description="phenotype persistence time of GFP+ cells">3000.0</persitence_timeHip>
		<fracResponse_green type="double" units="dimensionless" description="fraction of GFP+ cells with different motility bias (bias_greenResp)">0.5</fracResponse_green>
        <bias_greenResp type="double" units="dimensionless" description="motility bias for GFP + cells (for fracResponse_green)">0.5</bias_greenResp>
        <hypoxyprobe type="bool" description="mark with hypoxyprobe">true</hypoxyprobe>
		<sigma_Hp type="double" units="mmHg" description="oxygen pressure threshold to mark hypoxyprobe (&#62; sigma_T and &#60; sigma_H)">7.0</sigma_Hp>
		<speed_red type="double" units="micron/min" description="motility speed for DsRed+ cells">0.2938</speed_red>
		<perst_time_red type="double" units="min" description="motility persistence time for DsRed+ cells">15.0</perst_time_red>
		<bias_red type="double" units="dimensionless" description="motility bias for DsRed+ cells">0.1719</bias_red>
		<speed_green type="double" units="micron/min" description="motility speed for GFP+ cells">0.4755</speed_green>
		<perst_time_green type="double" units="min" description="motility persistence time for GFP+ cells">15.0</perst_time_green>
		<bias_green type="double" units="dimensionless" description="motility bias for GFP+ cells">0.1791</bias_green>  
        <tumor_radius type="double" units="micron" description="initial tumor radius">250.0</tumor_radius>
       <uptake_rate type="double" units="1/min" description="uptake rates for the oxygen">10.0</uptake_rate>
        <rate_KnToKp type="double" units="1/min" description="transition rate from Ki67- to Ki67+ (for saturated signal)">0.00363</rate_KnToKp>
		<rate_KpToKn type="double" units="1/min" description="transition rate from Ki67+ to Ki67-">0.00107</rate_KpToKn>
        <sigma_S type="double" units="mmHg" description="oxygen pressure threshold to signal proliferation saturation">38.0</sigma_S>
		<sigma_T type="double" units="mmHg" description="min oxygen pressure to signal proliferation">6.0</sigma_T>
        <sigma_H type="double" units="mmHg" description="oxygen pressure threshold to signal hypoxia">10.0</sigma_H>
        <production_rate type="double" units="1/min" description="protein production rate">4.8e-4</production_rate>
        <degradation_rate type="double" units="1/min" description="protein degradation rate">6.8e-5</degradation_rate>
    </user_parameters>
</PhysiCell_settings>
